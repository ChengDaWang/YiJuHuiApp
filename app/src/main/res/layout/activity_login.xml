<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginActivity">

    <ImageView
        android:id="@+id/loginImage"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.523"
        app:srcCompat="@mipmap/login" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/dp_160"
        android:layout_height="@dimen/dp_160"
        android:layout_marginStart="@dimen/dp_135"
        android:layout_marginEnd="@dimen/dp_137"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/loginImage"
        app:layout_constraintEnd_toEndOf="@+id/loginImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/loginImage"
        app:layout_constraintTop_toTopOf="@+id/loginImage"
        app:layout_constraintVertical_bias="0.124"
        app:srcCompat="@mipmap/head" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/rl_loginactivity"
        android:layout_width="@dimen/dp_280"
        android:layout_height="@dimen/dp_360"
        android:layout_marginStart="@dimen/dp_75"
        android:layout_marginEnd="@dimen/dp_77"
        android:layout_marginBottom="@dimen/dp_74"
        android:background="@drawable/corner"
        app:layout_constraintBottom_toBottomOf="@+id/loginImage"
        app:layout_constraintEnd_toEndOf="@+id/loginImage"
        app:layout_constraintStart_toStartOf="@+id/loginImage">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/dp_71"
            android:layout_height="@dimen/dp_73"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_84"
            android:layout_marginEnd="@dimen/dp_300"
            android:layout_marginBottom="@dimen/dp_203"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@android:drawable/ic_menu_myplaces" />

        <EditText
            android:id="@+id/userName"
            android:layout_width="@dimen/dp_197"
            android:layout_height="@dimen/dp_74"
            android:layout_marginTop="@dimen/dp_85"
            android:layout_marginEnd="@dimen/dp_9"
            android:layout_marginBottom="@dimen/dp_201"
            android:ems="10"
            android:hint="请输入用户名"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="@dimen/dp_71"
            android:layout_height="@dimen/dp_73"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_16"
            android:layout_marginEnd="@dimen/dp_204"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:srcCompat="@android:drawable/ic_menu_edit" />

        <EditText
            android:id="@+id/password"
            android:layout_width="@dimen/dp_197"
            android:layout_height="@dimen/dp_74"
            android:layout_marginTop="@dimen/dp_16"
            android:layout_marginEnd="@dimen/dp_9"
            android:layout_marginBottom="@dimen/dp_112"
            android:ems="10"
            android:hint="请输入密码"
            android:inputType="textPassword"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/userName" />

        <Button
            android:id="@+id/loginBT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_29"
            android:layout_marginEnd="@dimen/dp_177"
            android:layout_marginBottom="@dimen/dp_34"
            android:background="@drawable/button_corner"
            android:onClick="login"
            android:text="登陆"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/password" />

        <Button
            android:id="@+id/regies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_74"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_34"
            android:background="@drawable/button_corner"
            android:onClick="regTo"
            android:text="注册"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/loginBT"
            app:layout_constraintTop_toBottomOf="@+id/password"
            tools:ignore="OnClick" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>